<h2> Edit Companies </h2>

<%= form_for @company do |f| %>
<p>
<%= f.label :company_name %><br>
<%= f.text_field :name %>
</p>
<p>
<%= f.label :loaction %><br>
<%= f.text_field :location %>
</p>
<p>
<%= f.label :company_description %><br>
<%= f.text_area :description %>
</p>

<%= f.submit %>
<% end %>
